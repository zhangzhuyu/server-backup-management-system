<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper
    namespace="com.ly.cloud.backup.mapper.DemandFeedbackMapper">

    <resultMap id="BaseResultMap"
        type="com.ly.cloud.backup.po.DemandFeedbackPo">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="title" property="title" jdbcType="VARCHAR" />
        <result column="emergency_degree" property="emergency_degree" jdbcType="VARCHAR" />
        <result column="demand_type" property="demand_type" jdbcType="VARCHAR" />
        <result column="description" property="description" jdbcType="VARCHAR" />
        <result column="file_id" property="file_id" jdbcType="VARCHAR" />
        <result column="file_name" property="file_name" jdbcType="VARCHAR" />
        <result column="contact_name" property="contact_name" jdbcType="VARCHAR" />
        <result column="contact_way" property="contact_way" jdbcType="VARCHAR" />
        <result column="send_status" property="send_status" jdbcType="VARCHAR" />
        <result column="worker_id" property="worker_id" jdbcType="VARCHAR" />
        <result column="work_status" property="work_status" jdbcType="VARCHAR" />
        <result column="work_time" property="work_time" jdbcType="DATE" />
        <result column="result_feedback" property="result_feedback" jdbcType="VARCHAR" />
        <result column="operator_id" property="operator_id" jdbcType="VARCHAR" />
        <result column="operation_time" property="operation_time" jdbcType="DATE" />
        <result column="remark" property="remark" jdbcType="VARCHAR" />
    </resultMap>


    <sql id="Base_Column_List">
		id,
		title,
		emergency_degree,
		demand_type,
		description,
		file_id,
		file_name,
		contact_name,
		contact_way,
		send_status,
		worker_id,
		work_status,
		work_time,
		result_feedback,
		operator_id,
		operation_time,
		remark
    </sql>
    
    <resultMap id="DemandFeedbackMap"
               type="com.ly.cloud.backup.vo.DemandFeedbackVo" extends="BaseResultMap">
        
    </resultMap>

    
    <!-- 查询 -->
    <select id="select" resultMap="DemandFeedbackMap"
        parameterType="com.ly.cloud.backup.dto.DemandFeedbackDto">
        select
        <include refid="Base_Column_List" />
        from ly_ms_demand_feedback
        <where>
        <if test="dto.id != null and dto.id != '' ">
          and  ID = #{dto.id,jdbcType=BIGINT}
        </if>
        <if test="dto.title != null and dto.title != '' ">
          and  TITLE = #{dto.title,jdbcType=VARCHAR}
        </if>
        <if test="dto.emergency_degree != null and dto.emergency_degree != '' ">
          and  EMERGENCY_DEGREE = #{dto.emergency_degree,jdbcType=VARCHAR}
        </if>
        <if test="dto.demand_type != null and dto.demand_type != '' ">
          and  DEMAND_TYPE = #{dto.demand_type,jdbcType=VARCHAR}
        </if>
        <if test="dto.description != null and dto.description != '' ">
          and  DESCRIPTION = #{dto.description,jdbcType=VARCHAR}
        </if>
        <if test="dto.file_id != null and dto.file_id != '' ">
          and  FILE_ID = #{dto.file_id,jdbcType=VARCHAR}
        </if>
        <if test="dto.file_name != null and dto.file_name != '' ">
          and  FILE_NAME = #{dto.file_name,jdbcType=VARCHAR}
        </if>
        <if test="dto.contact_name != null and dto.contact_name != '' ">
          and  CONTACT_NAME = #{dto.contact_name,jdbcType=VARCHAR}
        </if>
        <if test="dto.contact_way != null and dto.contact_way != '' ">
          and  CONTACT_WAY = #{dto.contact_way,jdbcType=VARCHAR}
        </if>
        <if test="dto.send_status != null and dto.send_status != '' ">
          and  SEND_STATUS = #{dto.send_status,jdbcType=VARCHAR}
        </if>
        <if test="dto.worker_id != null and dto.worker_id != '' ">
          and  WORKER_ID = #{dto.worker_id,jdbcType=VARCHAR}
        </if>
        <if test="dto.work_status != null and dto.work_status != '' ">
          and  WORK_STATUS = #{dto.work_status,jdbcType=VARCHAR}
        </if>
        <if test="dto.work_time != null and dto.work_time != '' ">
          and  WORK_TIME = #{dto.work_time,jdbcType=DATE}
        </if>
        <if test="dto.result_feedback != null and dto.result_feedback != '' ">
          and  RESULT_FEEDBACK = #{dto.result_feedback,jdbcType=VARCHAR}
        </if>
        <if test="dto.operator_id != null and dto.operator_id != '' ">
          and  OPERATOR_ID = #{dto.operator_id,jdbcType=VARCHAR}
        </if>
        <if test="dto.operation_time != null and dto.operation_time != '' ">
          and  OPERATION_TIME = #{dto.operation_time,jdbcType=DATE}
        </if>
        <if test="dto.remark != null and dto.remark != '' ">
          and  REMARK = #{dto.remark,jdbcType=VARCHAR}
        </if>
        </where>
    </select>

</mapper>